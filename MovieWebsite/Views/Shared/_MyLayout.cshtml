<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <style type="text/css">
        .fancybox-margin {
            margin-right: 17px;
        }
    </style>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/Scripts")
</head>
<body style="overflow: visible" class="openform">
    @{
        Html.RenderAction("GetMenu", "Default");
    }
    @RenderBody()
    @{ 
        Html.RenderAction("GetFooter", "Default");
    }
</body>
</html>