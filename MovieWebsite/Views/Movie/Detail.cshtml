                                                                }

                                                            }
                                                            <p class="time">
                                                                @review.InitDate.Value.ToString("dd MMMM yyyy")
                                                            </p>
                                                        </div>
                                                    </div>
                                                </div>
                                                <p>@review.Content</p>
                                            </div>
                                        }
